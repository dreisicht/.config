// Platzieren Sie Ihre Tastenzuordnungen in dieser Datei, um die Standardwerte zu Ã¼berschreiben.
[
  {
    "command": "editor.action.moveLinesUpAction",
    "key": "alt+up",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "-editor.action.moveLinesUpAction",
    "key": "alt+up",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "editor.action.moveLinesDownAction",
    "key": "alt+down",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "-editor.action.moveLinesDownAction",
    "key": "alt+down",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "editor.action.copyLinesUpAction",
    "key": "shift+alt+up",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "-editor.action.copyLinesUpAction",
    "key": "shift+alt+up",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "editor.action.copyLinesDownAction",
    "key": "shift+alt+down",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "-editor.action.copyLinesDownAction",
    "key": "shift+alt+down",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "editor.action.insertCursorAbove",
    "key": "ctrl+up",
    "when": "editorTextFocus"
  },
  {
    "command": "-editor.action.insertCursorAbove",
    "key": "ctrl+alt+up",
    "when": "editorTextFocus"
  },
  {
    "command": "editor.action.insertCursorBelow",
    "key": "ctrl+down",
    "when": "editorTextFocus"
  },
  {
    "command": "-editor.action.insertCursorBelow",
    "key": "ctrl+alt+down",
    "when": "editorTextFocus"
  },
  {
    "command": "editor.fold",
    "key": "ctrl+shift+oem_4",
    "when": "editorTextFocus"
  },
  {
    "command": "-editor.fold",
    "key": "ctrl+alt+oem_4",
    "when": "editorTextFocus"
  },
  {
    "command": "workbench.action.showCommands",
    "key": "ctrl+shift+space"
  },
  {
    "command": "-workbench.action.showCommands",
    "key": "ctrl+shift+p"
  },
  {
    "command": "editor.debug.action.toggleLogPoint",
    "key": "ctrl+l"
  },
  {
    "command": "editor.action.commentLine",
    "key": "ctrl+[Backslash]",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "-editor.action.commentLine",
    "key": "ctrl+shift+7",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "workbench.action.tasks.build",
    "key": "f7"
  },
  {
    "command": "-workbench.action.tasks.build",
    "key": "ctrl+shift+b"
  },
  {
    "command": "-workbench.action.showCommands",
    "key": "f1"
  },
  {
    "command": "github.copilot.toggleCopilot",
    "key": "alt+oem_5"
  },
  {
    "command": "editor.action.inlineSuggest.trigger",
    "key": "f1",
    "when": "config.github.copilot.inlineSuggest.enable && editorTextFocus && !editorHasSelection && !inlineSuggestionsVisible"
  },
  {
    "command": "renameFile",
    "key": "f2",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "command": "-renameFile",
    "key": "enter",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "command": "-editor.action.inlineSuggest.trigger",
    "key": "alt+oem_5",
    "when": "config.github.copilot.inlineSuggest.enable && editorTextFocus && !editorHasSelection && !inlineSuggestionsVisible"
  },
  {
    "command": "editor.action.selectHighlights",
    "key": "ctrl+alt+d",
    "when": "editorFocus"
  },
  {
    "command": "-editor.action.selectHighlights",
    "key": "ctrl+shift+l",
    "when": "editorFocus"
  },
  {
    "command": "cursorWordPartRight",
    "key": "ctrl+alt+right"
  },
  {
    "command": "deleteWordPartLeft",
    "key": "ctrl+alt+backspace"
  },
  {
    "command": "deleteWordPartRight",
    "key": "alt+delete"
  },
  {
    "command": "-editor.action.smartSelect.expand",
    "key": "shift+alt+right",
    "when": "editorTextFocus"
  },
  {
    "command": "cursorWordPartRightSelect",
    "key": "ctrl+shift+alt+right"
  },
  {
    "command": "cursorWordPartLeftSelect",
    "key": "ctrl+shift+alt+left"
  },
  {
    "command": "workbench.action.navigateBack",
    "key": "alt+left",
    "when": "canNavigateBack"
  },
  {
    "command": "-workbench.action.navigateForward",
    "key": "ctrl+shift+-",
    "when": "canNavigateForward"
  },
  {
    "command": "cursorWordPartLeft",
    "key": "ctrl+alt+left"
  },
  {
    "command": "editor.action.smartSelect.expand",
    "key": "shift+alt+right"
  },
  {
    "command": "editor.action.removeBrackets",
    "key": "alt+backspace",
    "when": "editorTextFocus"
  },
  {
    "command": "-editor.action.removeBrackets",
    "key": "ctrl+alt+backspace",
    "when": "editorTextFocus"
  },
  {
    "command": "blender.runScript",
    "key": "ctrl+alt+r"
  },
  {
    "command": "copyFilePath",
    "key": "ctrl+shift+c",
    "when": "!editorFocus"
  },
  {
    "command": "-copyFilePath",
    "key": "shift+alt+c",
    "when": "!editorFocus"
  },
  {
    "command": "copyFilePath",
    "key": "ctrl+shift+c",
    "when": "editorFocus"
  },
  {
    "command": "-copyFilePath",
    "key": "shift+alt+c",
    "when": "editorFocus"
  },
  {
    "command": "copyRelativeFilePath",
    "key": "ctrl+shift+alt+c",
    "when": "!editorFocus"
  },
  {
    "command": "-copyRelativeFilePath",
    "key": "ctrl+k ctrl+shift+c",
    "when": "!editorFocus"
  },
  {
    "command": "copyRelativeFilePath",
    "key": "ctrl+shift+alt+c",
    "when": "editorFocus"
  },
  {
    "command": "-copyRelativeFilePath",
    "key": "ctrl+k ctrl+shift+c",
    "when": "editorFocus"
  },
  {
    "command": "editor.action.blockComment",
    "key": "ctrl+shift+oem_2",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "-editor.action.blockComment",
    "key": "shift+alt+a",
    "when": "editorTextFocus && !editorReadonly"
  }
]
